<view class="serch_result">
	<view class="result-content u-ul ">
		<view class="u-li u-clearfix">
			<text class=" u-fl-l">标的股票</text>
			<text class="u-fl-r">{{infoData.name}} {{infoData.code}}</text>
		</view>
		<view class="u-li u-clearfix">
			<text class="u-fl-l">名义本金</text>
			<text class="u-fl-r">{{infoData.benjin}}</text>
		</view>
		<view class="u-li u-clearfix">
			<text class="u-fl-l">合约期限</text>
			<text class="u-fl-r">{{infoData.qixian}}</text>
		</view>
		<view class="u-li u-clearfix">
			<text class="u-fl-l">起始日期</text>
			<text class="u-fl-r">{{infoData.startTime}}</text>
		</view>
		<view class="u-li u-clearfix">
			<text class="u-fl-l">到期日期</text>
			<text class="u-fl-r">{{infoData.endTime}}</text>
		</view>
	</view>
	<view class="profit-price">
		<text class="u-fl-l">盈利定价</text>
		<text class="percents u-fl-r">{{infoData.price}}</text>
	</view>
	<view class="button-wrap">
		<button class="consult-btn" bindtap="searchAgin">继续询价</button>
	</view>
</view>

